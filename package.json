{
  "name": "react-native-detox-cucumber",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "test": "jest"
  },
  "keywords": [
    "react-native",
    "detox",
    "cucumber",
    "e2e",
    "testing",
    "bdd"
  ],
  "dependencies": {
    "detox": "^14.7.1",
    "mocha": "^6.2.2",
    "react": "16.8.6",
    "react-native": "0.60.6"
  },
  "devDependencies": {
    "@babel/core": "^7.5.0",
    "@babel/runtime": "^7.5.0",
    "@react-native-community/eslint-config": "^0.0.3",
    "babel-jest": "^24.1.0",
    "cucumber": "^6.0.5",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "0.54.1",
    "react-test-renderer": "16.8.6"
  },
  "jest": {
    "preset": "react-native"
  },
  "detox": {
    "configurations": {
      "ios.sim.release": {
        "binaryPath": "ios/build/Build/Products/Release-iphonesimulator/e2eReactNative60.app",
        "build": "export RCT_NO_LAUNCH_PACKAGER=true && xcodebuild -workspace ios/e2eReactNative60.xcworkspace -UseNewBuildSystem=NO -scheme e2eReactNative60 -configuration Release -sdk iphonesimulator -derivedDataPath ios/build -quiet",
        "type": "ios.simulator",
        "device": {
          "type": "iPhone 11 Pro"
        }
      },
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/e2eReactNative60.app",
        "build": "xcodebuild -workspace ios/e2eReactNative60.xcworkspace -UseNewBuildSystem=NO -scheme e2eReactNative60 -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "device": {
          "type": "iPhone 11 Pro"
        }
      },
      "ios.none": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/e2eReactNative60.app",
        "build": "xcodebuild -workspace ios/e2eReactNative60.xcworkspace -UseNewBuildSystem=NO -scheme e2eReactNative60 -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.none",
        "device": {
          "type": "iPhone 11 Pro"
        },
        "session": {
          "server": "ws://localhost:8099",
          "sessionId": "com.wix.demo.react.native"
        }
      },
      "android.emu.debug": {
        "binaryPath": "android/app/build/outputs/apk/debug/app-debug.apk",
        "build": "cd android ; ./gradlew assembleDebug assembleAndroidTest -DtestBuildType=debug ; cd -",
        "type": "android.emulator",
        "device": {
          "avdName": "Nexus_5X_API_26_2"
        }
      },
      "android.emu.release": {
        "binaryPath": "android/app/build/outputs/apk/release/app-release.apk",
        "build": "cd android ; ./gradlew assembleRelease assembleAndroidTest -DtestBuildType=release ; cd -",
        "type": "android.emulator",
        "device": {
          "avdName": "Nexus_5X_API_26_2"
        }
      }
    }
  }
}
